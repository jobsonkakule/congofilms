{{ form_start(form, {'attr': {'class': 'filter js-filter-form'}}) }}
    <div class="spinner-border js-loading" role="status" aria-hidden="true" style="display: none;">
        <div class="sr-only">Chargement...</div>
    </div>
    {{ form_row(form.q) }}
    <h4>Catégorie</h4>
    {{ form_row(form.category) }}
    <h4>Priorité</h4>
    <div class="row">
        <div class="col-md-6">
            {{ form_row(form.min) }}
        </div>
        <div class="col-md-6">
            {{ form_row(form.max) }}
        </div>
    </div>
    <div class="ml-3 mr-3" id="score-slider" data-min="{{ min }}" data-max="{{ max }}"></div>
    <br>
    <div style="display: none;">
        {{ form_row(form.online) }}
    </div>
    <button type="submit" class="btn btn-primary w-100">Filtrer</button>
{{ form_end(form) }}